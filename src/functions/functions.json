{
  "allowCustomDataForDataTypeAny": true,
  "functions": [
    {
      "id": "CALCULATE",
      "name": "CALCULATE",
      "description": "Calculate surplus lines tax for a given state and premium amount",
      "helpUrl": "https://surpluslinesapi.com/excel/#calculate",
      "parameters": [
        {
          "name": "state",
          "description": "State name (e.g., \"Texas\", \"California\") or abbreviation (e.g., \"TX\", \"CA\")",
          "type": "string",
          "dimensionality": "scalar"
        },
        {
          "name": "premium",
          "description": "Premium amount in dollars",
          "type": "number",
          "dimensionality": "scalar"
        }
      ],
      "result": {
        "type": "any",
        "dimensionality": "scalar"
      },
      "options": {
        "requiresAddress": false,
        "stream": false,
        "volatile": false,
        "requiresParameterAddresses": false
      }
    },
    {
