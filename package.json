{
  "name": "surplus-lines-tax-excel-addin",
  "version": "1.0.0",
  "description": "Excel Add-in for Surplus Lines Tax calculations - custom functions for all 50 US states",
  "main": "src/functions/functions.js",
  "author": "Underwriters Technologies <support@undtec.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/undtec/surplus-lines-excel-addin"
  },
  "keywords": [
    "excel",
    "office-add-in",
    "custom-functions",
    "surplus-lines",
    "insurance",
    "tax-calculator",
    "e&s-insurance"
  ],
  "scripts": {
    "build": "webpack --mode production",
    "build:dev": "webpack --mode development",
    "dev-server": "webpack serve --mode development",
    "start": "npm run dev-server",
    "start:desktop": "npm run sideload && npm run dev-server",
    "start:web": "office-addin-debugging start manifest.xml web",
    "sideload": "node scripts/sideload.js manifest.dev.xml",
    "sideload:prod": "node scripts/sideload.js manifest.xml",
    "unload": "node scripts/unload.js",
    "stop": "office-addin-debugging stop manifest.xml",
    "validate": "office-addin-manifest validate manifest.xml",
    "lint": "eslint src/**/*.js",
    "test": "jest"
  },
  "dependencies": {
    "core-js": "^3.36.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "babel-loader": "^9.1.3",
    "copy-webpack-plugin": "^12.0.2",
    "eslint": "^8.57.0",
    "html-webpack-plugin": "^5.6.0",
    "jest": "^29.7.0",
    "office-addin-debugging": "^5.1.0",
    "office-addin-dev-certs": "^1.13.0",
    "office-addin-manifest": "^1.13.0",
    "webpack": "^5.90.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
